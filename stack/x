public class Stack_class {
    static class Node {
        int data;
        Node next;

        public Node(int data){
            this.data = data;
            this.next = null;
        }
    }
    
    static class Stack {
        private Node head;

        public boolean isEmpty(){
            return head == null;
        }

        public void push(int data){
            Node newNode = new Node(data);
            newNode.next = head;
            head = newNode;
        }

        public int pop(){
            if (isEmpty()) {
                return -1;
            }

            int top = head.data;
            head = head.next;
            return top;
        }

        public int peek(){
            if (isEmpty()) {
                return -1;
            }
            return head.data;
        }
    }

    // Main method
    public static void main(String[] args){
        Stack s = new Stack();
        s.push(10);
        s.push(20);
        s.push(30);

        System.out.println("Popping all elements:");
        while (!s.isEmpty()){
            System.out.println(s.peek());
            s.pop();
        }

        System.out.println("Top element is: " + s.peek()); // Should print -1
        System.out.println("Popped element is: " + s.pop()); // Should print -1
        System.out.println("Top element after pop is: " + s.peek()); // Should print -1
        System.out.println("Is stack empty? " + s.isEmpty()); // Should print true
    }
}
